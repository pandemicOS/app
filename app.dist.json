{
  "expo": {
    "name": "PandemicOS - Coronavirus",
    "slug": "pandemic-os-coronavirus",
    "version": "0.1.0",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.pandemicos.coronavirus",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.pandemicos.coronavirus",
      "versionCode": 1
    },
    "icon": "./icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png"
    },
    "privacy": "unlisted",
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    }
  }
}
